						"id": "92dd317b-3c60-4e99-97f2-40dc37a896b8",
						"type": "text/javascript",
						"exec": [
							"let i = +environment.iteration;",
							"let B = +environment.N;",
							"",
							"// Check status code",
							"pm.test(\"Status code is 200\", function () {",
							"   pm.response.to.have.status(200); ",
							"});",
							"",
							"",
							"",
							"if (i == B)",
							"{",
							"        postman.setNextRequest('null'); // breaks the loop",
							"}",
							"        ",
							"else",
							"{",
							"        postman.setNextRequest('Get All Transactions and Define Variables Copy');",
							"        i=i+1;",
							"        postman.setEnvironmentVariable(\"iteration\", i);",
							"        ",
							"}"
						]
					}
